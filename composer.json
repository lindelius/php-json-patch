{
    "name": "lindelius/php-json-patch",
    "description": "A zero-dependency PHP implementation of JSON Patch",
    "require": {
        "php": "^7.4||^8.0",
        "ext-json": "*",
        "symfony/polyfill-php81": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Lindelius\\JsonPatch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lindelius\\JsonPatch\\Tests\\": "tests"
        }
    }
}
